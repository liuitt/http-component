{
  "description": "The Liuitt Framework's Http Component.",
  "keywords": ["components", "liuitt"],
  "license": "MIT",
  "homepage": "http://liuitt.com",
  "support": {
    "issues": "https://github.com/liuitt/http-component/issues",
    "source": "https://github.com/liuitt/http-component"
  },
  "authors": [
    {
      "name": "Gilberto Albino",
      "email": "gilbertoalbino@gmail.com"
    }
  ],
  "config": {
    "vendor-dir": "vendor/liuitt/components"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "liuitt/http",
        "version": "master",
        "source": {
          "url": "https://github.com/liuitt/http-component",
          "type": "git",
          "reference": "master"
        }
      },
      "require": {
        "composer/installers": "~1.0"
      }
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Liuitt\\": "src/Liuitt"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}


